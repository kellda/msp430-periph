<module HW_revision="2.0" XML_version="1" description="JSTATE decoding" id="Hidden" hidden="true">

   <!-- JSTATE register definition for the following devices
	MSP430FR5870
	MSP430FR5970
	MSP430FR5871
	MSP430FR5971
	MSP430FR5872
	MSP430FR5972
	MSP430FR6820
	MSP430FR6920
	MSP430FR6821
	MSP430FR6921
	MSP430FR6822
	MSP430FR6922
	MSP430FR6870
	MSP430FR6970
	MSP430FR6871
	MSP430FR6971
	MSP430FR6872
	MSP430FR6972
	MSP430FR58721
	MSP430FR59721
	MSP430FR68221
	MSP430FR69221
	MSP430FR68721
	MSP430FR69721
  -->
   <register acronym="JSTATE2" description="Digital JTAG State Register" id="JSTATE2" offset="0x0" width="64">

    <!-- Next bits show peripheral activity. Bits with description="NA" are not associated to a peripheral. -->
      <bitfield begin="31" description="FRAM" end="31" id="MODACT0" range="" resetval="0" rwaccess="R" width="1" />
      <bitfield begin="30" description="MPY"  end="30" id="MODACT1" range="" resetval="0" rwaccess="R" width="1" />
      <bitfield begin="29" description="WDT"  end="29" id="MODACT2" range="" resetval="0" rwaccess="R" width="1" />
      <bitfield begin="28" description="RTC"  end="28" id="MODACT3" range="" resetval="0" rwaccess="R" width="1" />
      <bitfield begin="27" description="ADC"  end="27" id="MODACT4" range="" resetval="0" rwaccess="R" width="1" />
      <bitfield begin="26" description="REF"  end="26" id="MODACT5" range="" resetval="0" rwaccess="R" width="1" />
      <bitfield begin="25" description="COMP" end="25" id="MODACT6" range="" resetval="0" rwaccess="R" width="1" />
      <bitfield begin="24" description="AES"  end="24" id="MODACT7" range="" resetval="0" rwaccess="R" width="1" />
      <bitfield begin="23" description="eUSCI_A0" end="23" id="MODACT8" range="" resetval="0" rwaccess="R" width="1" />
      <bitfield begin="22" description="eUSCI_A1" end="22" id="MODACT9" range="" resetval="0" rwaccess="R" width="1" />
      <bitfield begin="21" description="eUSCI_B0" end="21" id="MODACT10" range="" resetval="0" rwaccess="R" width="1" />
      <bitfield begin="20" description="eUSCI_B1" end="20" id="MODACT11" range="" resetval="0" rwaccess="R" width="1" />
      <bitfield begin="19" description="TB0"  end="19" id="MODACT12" range="" resetval="0" rwaccess="R" width="1" />
      <bitfield begin="18" description="TA0"  end="18" id="MODACT13" range="" resetval="0" rwaccess="R" width="1" />
      <bitfield begin="17" description="TA1"  end="17" id="MODACT14" range="" resetval="0" rwaccess="R" width="1" />
      <bitfield begin="16" description="TA2"  end="16" id="MODACT15" range="" resetval="0" rwaccess="R" width="1" />
      <bitfield begin="15" description="TA3"  end="15" id="MODACT16" range="" resetval="0" rwaccess="R" width="1" />
      <bitfield begin="14" description="LCD"   end="14" id="MODACT17" range="" resetval="0" rwaccess="R" width="1" />
      <bitfield begin="8" description="DMA"   end="8" id="MODDMA" range="" resetval="0" rwaccess="R" width="1" />

   </register>
</module>
